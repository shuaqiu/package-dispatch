    <!-- {2}: 寄件人的电话 -->
    <!-- {3}: 寄件人的公司 -->
    <!-- {4}: 寄件人的地址 -->
    <!-- {5}: 物品名称 -->
    <!-- {6}: 物品数量 -->
    <entry key="order.notify.receiver">你有新的快件, 验证码为: {0}</entry>
